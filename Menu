-- Fun√ß√£o para criar sec√ß√µes
local function criarSecao(tituloTexto)
	local secao = Instance.new("Frame", scroll)
	secao.Size = UDim2.new(1, -10, 0, 50)
	secao.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	secao.AutomaticSize = Enum.AutomaticSize.Y
	Instance.new("UICorner", secao).CornerRadius = UDim.new(0, 6)

	local titulo = Instance.new("TextLabel", secao)
	titulo.Size = UDim2.new(1, 0, 0, 30)
	titulo.Text = tituloTexto
	titulo.BackgroundTransparency = 1
	titulo.TextColor3 = Color3.new(1, 1, 1)
	titulo.Font = Enum.Font.GothamBold
	titulo.TextSize = 18

	return secao
end

-- Criar as sec√ß√µes (apenas visuais por agora)
