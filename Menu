-- Fun√ß√£o para ativar noclip (permite atravessar paredes)
local function setNoclip(state)
	local character = player.Character or player.CharacterAdded:Wait()
	for _, part in pairs(character:GetChildren()) do
		if part:IsA("BasePart") then
			part.CanCollide = not state
		end
	end
end

local flyAtivo = false
local flyBotao = Instance.new("TextButton", secaoPrincipal)
flyBotao.Size = UDim2.new(1, -20, 0, 30)
flyBotao.Position = UDim2.new(0, 10, 0, 40)
flyBotao.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
flyBotao.TextColor3 = Color3.new(1, 1, 1)
flyBotao.Font = Enum.Font.Gotham
flyBotao.TextSize = 14
flyBotao.Text = "Fly (Noclip)"
flyBotao.TextWrapped = true
Instance.new("UICorner", flyBotao).CornerRadius = UDim.new(0, 6)

local stopFlyFunc = nil

local function ativarFly()
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

	setNoclip(true)  -- ativa noclip

	local bodyGyro = Instance.new("BodyGyro")
	bodyGyro.P = 9e4
	bodyGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
	bodyGyro.CFrame = humanoidRootPart.CFrame
	bodyGyro.Parent = humanoidRootPart

	local bodyVelocity = Instance.new("BodyVelocity")
	bodyVelocity.Velocity = Vector3.new(0, 0, 0)
	bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
	bodyVelocity.Parent = humanoidRootPart

	local flying = true

	local connection = game:GetService("RunService").RenderStepped:Connect(function()
		if not flying then return end
		local camera = workspace.CurrentCamera
		local moveDirection = player.Character:FindFirstChildOfClass("Humanoid").MoveDirection
		local camLookVector = camera.CFrame.LookVector
		local speed = 60

		if moveDirection.Magnitude > 0 then
			bodyVelocity.Velocity = camLookVector.Unit * speed
		else
			bodyVelocity.Velocity = Vector3.new(0, 0, 0)
		end

		bodyGyro.CFrame = camera.CFrame
	end)

	return function()
		flying = false
		connection:Disconnect()
		bodyGyro:Destroy()
		bodyVelocity:Destroy()
		setNoclip(false) -- desativa noclip
	end
end

flyBotao.MouseButton1Click:Connect(function()
	flyAtivo = not flyAtivo
	if flyAtivo then
		stopFlyFunc = ativarFly()
		flyBotao.TextColor3 = Color3.fromRGB(0, 255, 0) -- verde ativo
	else
		if stopFlyFunc then stopFlyFunc() end
		flyBotao.TextColor3 = Color3.new(1, 1, 1) -- branco inativo
	end
end)

-- Bot√£o Invis√≠vel
local invisivelAtivo = false
local invisivelBotao = Instance.new("TextButton", secaoPrincipal)
invisivelBotao.Size = UDim2.new(1, -20, 0, 30)
invisivelBotao.Position = UDim2.new(0, 10, 0, 80) -- um pouco abaixo do bot√£o Fly
invisivelBotao.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
invisivelBotao.TextColor3 = Color3.new(1, 1, 1)
invisivelBotao.Font = Enum.Font.Gotham
invisivelBotao.TextSize = 14
invisivelBotao.Text = "Invis√≠vel"
invisivelBotao.TextWrapped = true
Instance.new("UICorner", invisivelBotao).CornerRadius = UDim.new(0, 6)

local function setInvisivel(state)
    local character = player.Character or player.CharacterAdded:Wait()

    for _, part in pairs(character:GetChildren()) do
        if part:IsA("BasePart") then
            part.Transparency = state and 1 or 0
            part.CanCollide = not state
        elseif part:IsA("Decal") or part:IsA("Texture") then
            part.Transparency = state and 1 or 0
        elseif part:IsA("Accessory") then
            local handle = part:FindFirstChild("Handle")
            if handle then
                handle.Transparency = state and 1 or 0
                handle.CanCollide = not state
            end
            for _, decal in pairs(part:GetChildren()) do
                if decal:IsA("Decal") or decal:IsA("Texture") then
                    decal.Transparency = state and 1 or 0
                end
            end
        end
    end
end

invisivelBotao.MouseButton1Click:Connect(function()
    invisivelAtivo = not invisivelAtivo
    setInvisivel(invisivelAtivo)
    if invisivelAtivo then
        invisivelBotao.TextColor3 = Color3.fromRGB(0, 255, 0) -- verde ligado
    else
        invisivelBotao.TextColor3 = Color3.new(1, 1, 1) -- branco desligado
    end
end)

local function setInvisivel(state)
    local character = player.Character or player.CharacterAdded:Wait()

    for _, part in pairs(character:GetChildren()) do
        if part:IsA("BasePart") then
            -- Torna invis√≠vel todas as partes f√≠sicas, incluindo cabe√ßa
            part.Transparency = state and 1 or 0
            part.CanCollide = not state
        elseif part:IsA("Decal") or part:IsA("Texture") then
            -- Face e outras texturas
            part.Transparency = state and 1 or 0
        elseif part:IsA("Accessory") then
            -- Para acess√≥rios como cabelo, chap√©us, etc.
            local handle = part:FindFirstChild("Handle")
            if handle then
                handle.Transparency = state and 1 or 0
                handle.CanCollide = not state
            end
            -- Tamb√©m ocultar decals dentro do acess√≥rio
            for _, decal in pairs(part:GetChildren()) do
                if decal:IsA("Decal") or decal:IsA("Texture") then
                    decal.Transparency = state and 1 or 0
                end
            end
        end
    end
end

-- Bot√£o God Mode
local godModeAtivo = false
local godModeBotao = Instance.new("TextButton", secaoPrincipal)
godModeBotao.Size = UDim2.new(1, -20, 0, 30)
godModeBotao.Position = UDim2.new(0, 10, 0, 120) -- abaixo do bot√£o invis√≠vel
godModeBotao.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
godModeBotao.TextColor3 = Color3.new(1, 1, 1)
godModeBotao.Font = Enum.Font.Gotham
godModeBotao.TextSize = 14
godModeBotao.Text = "God Mode"
godModeBotao.TextWrapped = true
Instance.new("UICorner", godModeBotao).CornerRadius = UDim.new(0, 6)

local godModeConexao = nil

godModeBotao.MouseButton1Click:Connect(function()
    godModeAtivo = not godModeAtivo
    
    if godModeAtivo then
        godModeBotao.TextColor3 = Color3.fromRGB(0, 255, 0)
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
            -- Mant√©m a vida sempre alta
            godModeConexao = game:GetService("RunService").Heartbeat:Connect(function()
                if humanoid and humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                end
            end)
        end
    else
        godModeBotao.TextColor3 = Color3.new(1,1,1)
        if godModeConexao then
            godModeConexao:Disconnect()
            godModeConexao = nil
        end
        -- Poderias resetar para valores normais aqui, mas cuidado para n√£o causar erros
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.MaxHealth = 100
            if humanoid.Health > 100 then
                humanoid.Health = 100
            end
        end
    end
end)


-- Bot√£o Anti Ban
local antiBanAtivo = false
local antiBanBotao = Instance.new("TextButton", secaoPrincipal)
antiBanBotao.Size = UDim2.new(1, -20, 0, 30)
antiBanBotao.Position = UDim2.new(0, 10, 0, 160) -- abaixo do bot√£o God Mode
antiBanBotao.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
antiBanBotao.TextColor3 = Color3.new(1, 1, 1)
antiBanBotao.Font = Enum.Font.Gotham
antiBanBotao.TextSize = 14
antiBanBotao.Text = "Anti Ban / Kick"
antiBanBotao.TextWrapped = true
Instance.new("UICorner", antiBanBotao).CornerRadius = UDim.new(0, 6)

local originalKick = player.Kick
local originalDestroy = player.Destroy

antiBanBotao.MouseButton1Click:Connect(function()
    antiBanAtivo = not antiBanAtivo
    
    if antiBanAtivo then
        antiBanBotao.TextColor3 = Color3.fromRGB(0, 255, 0)
        -- Sobrescreve m√©todos para impedir kick e destroy
        player.Kick = function() end
        player.Destroy = function() end
    else
        antiBanBotao.TextColor3 = Color3.new(1,1,1)
        -- Restaura m√©todos originais
        player.Kick = originalKick
        player.Destroy = originalDestroy
    end
end)

-- Bot√£o Stamina Infinita
local staminaAtiva = false
local staminaBotao = Instance.new("TextButton", secaoPrincipal)
staminaBotao.Size = UDim2.new(1, -20, 0, 30)
staminaBotao.Position = UDim2.new(0, 10, 0, 200) -- abaixo do bot√£o Anti Ban
staminaBotao.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
staminaBotao.TextColor3 = Color3.new(1, 1, 1)
staminaBotao.Font = Enum.Font.Gotham
staminaBotao.TextSize = 14
staminaBotao.Text = "Stamina Infinita"
staminaBotao.TextWrapped = true
Instance.new("UICorner", staminaBotao).CornerRadius = UDim.new(0, 6)

local staminaConexao = nil

staminaBotao.MouseButton1Click:Connect(function()
    staminaAtiva = not staminaAtiva
    
    if staminaAtiva then
        staminaBotao.TextColor3 = Color3.fromRGB(0, 255, 0)
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            staminaConexao = game:GetService("RunService").Heartbeat:Connect(function()
                -- Muitas vezes a stamina est√° ligada a Humanoid, mas depende do jogo
                if humanoid:FindFirstChild("Stamina") then
                    humanoid.Stamina.Value = humanoid.Stamina.MaxValue
                end
                -- Se for diferente, tens de me dizer o nome ou onde est√° a stamina no jogo
            end)
        end
    else
        staminaBotao.TextColor3 = Color3.new(1, 1, 1)
        if staminaConexao then
            staminaConexao:Disconnect()
            staminaConexao = nil
        end
    end
end)


-- Bot√£o Speed
local speedAtivo = false
local speedBotao = Instance.new("TextButton", secaoPrincipal)
speedBotao.Size = UDim2.new(1, -20, 0, 30)
speedBotao.Position = UDim2.new(0, 10, 0, 240) -- abaixo do bot√£o Stamina
speedBotao.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
speedBotao.TextColor3 = Color3.new(1, 1, 1)
speedBotao.Font = Enum.Font.Gotham
speedBotao.TextSize = 14
speedBotao.Text = "Speed"
speedBotao.TextWrapped = true
Instance.new("UICorner", speedBotao).CornerRadius = UDim.new(0, 6)

local velocidadePadrao = 16
local velocidadeExtra = 50
local humanoidSpeedConexao = nil

speedBotao.MouseButton1Click:Connect(function()
    speedAtivo = not speedAtivo
    
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    
    if speedAtivo then
        speedBotao.TextColor3 = Color3.fromRGB(0, 255, 0)
        if humanoid then
            velocidadePadrao = humanoid.WalkSpeed
            humanoid.WalkSpeed = velocidadeExtra
        end
    else
        speedBotao.TextColor3 = Color3.new(1, 1, 1)
        if humanoid then
            humanoid.WalkSpeed = velocidadePadrao
        end
    end
end)


